  <section-def-macros>
    <def-macro n="f_enviaa" npar="1">
      <choose>
	<when>
	  <test>
	    <and>
	      <equal>
		<var n="valverb"/>
		<lit v="2"/>
	      </equal>
	      <or>
		<equal>
		  <clip pos="1" side="sl" part="a_npant"/>
		  <lit-tag v="np.ant"/>
		</equal>
		<in caseless="yes">
		  <clip pos="1" side="tl" part="lem"/>
		  <list n="huma"/>
		</in>
	      </or>
	    </and>
	  </test>
	  <out>
	    <lu>
	      <lit v="a"/>
	      <lit-tag v="pr"/>
	    </lu>
	    <b/>
	    
	  </out>
	</when>
      </choose>
      <let>
	<var n="valverb"/>
	<lit v=""/>
      </let>
    </def-macro>


    <def-macro n="marca_no" npar="1">
      <choose>
	<when>
	  <test>
	    <equal caseless="yes">
	      <clip pos="1" side="sl" part="lem"/>
