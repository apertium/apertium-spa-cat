        <match tags="*.*"/>
        <match tags="adv"/>
        <match tags="preadv"/>
        <match tags="cm"/>
        <match tags="pr"/>
        <match tags="cnjcoo"/>
      </or>
    </repeat>
    <match lemma="agalla" tags="n.*"><select lemma="ganya"/></match>
  </rule>
   

  <rule weight="2">
    <match lemma="agalla" tags="n.*"><select lemma="ganya"/></match>
