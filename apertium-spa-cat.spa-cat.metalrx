  </rule>

  <rule weight="1">
    <match lemma="atizar" tags="vblex.*"><select lemma="atiar"/></match>
    <repeat from="0" upto="5">
      <or>
        <match tags="*.*"/>
        <match tags="adv"/>
        <match tags="preadv"/>
        <match tags="cm"/>
        <match tags="pr"/>
        <match tags="cnjcoo"/>
