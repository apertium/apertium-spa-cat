<?xml version="1.0" encoding="UTF-8"?>

<metalrx>
  <def-macros>

  <def-macro name="toponims_article">
    <rule weight="0.5">
      <match lemma="{{p1}}"><select lemma="{{p3}}"/></match>
    </rule>
    <rule weight="1">
      <or>
        <match tags="det.*"/>
        <match tags="adj.*"/>
        <match tags="vblex.pp.*"/>
        <match tags="n.*"/>
        <match tags="np.*"/>
      </or>
      <match lemma="{{p1}}"><select lemma="{{p2}}"/></match>
    </rule>
  </def-macro>

	<def-macro name="noms_propis">
	  <rule weight="0.6">
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p1}}"/></match>
	  </rule>
	  <rule weight="1.0">
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
		 <match lemma="de" tags="pr"/>
     <repeat from="0" upto="1"><match tags="det.def.*"/></repeat>
		 <or><match tags="np.loc"/><match tags="np.top.*"/></or>
	  </rule>
	  <rule weight="1.0">
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
		 <match tags="det.*"/>
		 <match tags="adj.*"/>
	  </rule>
	  <rule weight="1.0">
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
		 <or>
		   <match tags="num.*"/>
		   <match lemma="I" tags="adj.*"/>
		   <match lemma="II" tags="adj.*"/>
		   <match lemma="III" tags="adj.*"/>
		   <match lemma="IV" tags="adj.*"/>
		   <match lemma="V" tags="adj.*"/>
		   <match lemma="VI" tags="adj.*"/>
		   <match lemma="VII" tags="adj.*"/>
		   <match lemma="VIII" tags="adj.*"/>
		   <match lemma="IX" tags="adj.*"/>
		   <match lemma="X" tags="adj.*"/>
		   <match lemma="XI" tags="adj.*"/>
		   <match lemma="XII" tags="adj.*"/>
		   <match lemma="XIII" tags="adj.*"/>
		   <match lemma="XIV" tags="adj.*"/>
		   <match lemma="XV" tags="adj.*"/>
		   <match lemma="XVI" tags="adj.*"/>
		   <match lemma="XVII" tags="adj.*"/>
		   <match lemma="XVIII" tags="adj.*"/>
		   <match lemma="XIX" tags="adj.*"/>
		   <match lemma="XX" tags="adj.*"/>
		   <match lemma="XXI" tags="adj.*"/>
		   <match lemma="XXII" tags="adj.*"/>
		   <match lemma="XXIII" tags="adj.*"/>
		   <match lemma="XXIV" tags="adj.*"/>
		   <match lemma="XXV" tags="adj.*"/>
		   <match lemma="XXVI" tags="adj.*"/>
		   <match lemma="XXVII" tags="adj.*"/>
		   <match lemma="XXVIII" tags="adj.*"/>
		   <match lemma="XXIX" tags="adj.*"/>
		   <match lemma="XXX" tags="adj.*"/>
		 </or>
	  </rule>
	  <rule weight="1.0">
		 <or>
		   <match lemma="papa" tags="n.*"/>
		   <match lemma="obispo" tags="n.*"/>
		   <match lemma="arzobispo" tags="n.*"/>
		   <match lemma="santo" tags="n.*"/>
		   <match lemma="santo" tags="adj.*"/>
		   <match lemma="apóstol" tags="n.*"/>
		   <match lemma="emperador" tags="n.*"/>
		   <match lemma="rey" tags="n.*"/>
		   <match lemma="zar" tags="n.*"/>
		   <match lemma="virrey" tags="n.*"/>
		   <match lemma="regente" tags="n.*"/>
		   <match lemma="príncipe" tags="n.*"/>
		   <match lemma="infante" tags="n.*"/>
		   <match lemma="conde" tags="n.*"/>
		   <match lemma="vizconde" tags="n.*"/>
		   <match lemma="duque" tags="n.*"/>
		   <match lemma="archiduque" tags="n.*"/>
		   <match lemma="marqués" tags="n.*"/>
		   <match lemma="barón" tags="n.*"/>
		 </or>
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
	  </rule>
	  <rule weight="1.0">
		 <or>
		   <match lemma="papado" tags="n.*"/>
		   <match lemma="pontificado" tags="n.*"/>
		   <match lemma="imperio" tags="n.*"/>
		   <match lemma="reino" tags="n.*"/>
		   <match lemma="reinado" tags="n.*"/>
		   <match lemma="regencia" tags="n.*"/>
		 </or>
		 <match lemma="de" tags="pr"/>
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
	  </rule>
	</def-macro>

  <def-macro name="verbs_reflexius"> <!-- traducció del verb si va acompanyat per un pronom reflexiu -->
    <rule weight="2">
      <match tags="prn.pro.p1.mf.sg"/>
      <match lemma="{{p1}}" tags="vblex.*.p1.sg"><select lemma="{{p2}}"/></match>
    </rule>
    <rule weight="2">
      <match tags="prn.pro.p2.mf.sg"/>
      <match lemma="{{p1}}" tags="vblex.*.p2.sg"><select lemma="{{p2}}"/></match>
    </rule>
    <rule weight="1">
      <match tags="prn.pro.ref.p3.mf.sp"/>
      <match lemma="{{p1}}" tags="vblex.*.p3.sg"><select lemma="{{p2}}"/></match>
    </rule>
    <rule weight="2">
      <match tags="prn.pro.p1.mf.pl"/>
      <match lemma="{{p1}}" tags="vblex.*.p1.pl"><select lemma="{{p2}}"/></match>
    </rule>
    <rule weight="2">
      <match tags="prn.pro.p2.mf.pl"/>
      <match lemma="{{p1}}" tags="vblex.*.p2.pl"><select lemma="{{p2}}"/></match>
    </rule>
    <rule weight="2">
      <match tags="prn.pro.ref.p3.mf.sp"/>
      <match lemma="{{p1}}" tags="vblex.*.p3.pl"><select lemma="{{p2}}"/></match>
    </rule>

    <rule weight="2">
      <match tags="prn.pro.p1.mf.sg"/>
      <match tags="vbmod.*.p1.sg"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
    </rule>
    <rule weight="2">
      <match tags="prn.pro.p2.mf.sg"/>
      <match tags="vbmod.*.p2.sg"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
    </rule>
      <rule weight="1">
      <match tags="prn.pro.ref.p3.mf.sp"/>
      <match tags="vbmod.*.p3.sg"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
    </rule>
    <rule weight="2">
      <match tags="prn.pro.p1.mf.pl"/>
      <match tags="vbmod.*.p1.pl"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
    </rule>
    <rule weight="2">
      <match tags="prn.pro.p2.mf.pl"/>
      <match tags="vbmod.*.p2.pl"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
    </rule>
      <rule weight="2">
      <match tags="prn.pro.ref.p3.mf.sp"/>
      <match tags="vbmod.*.p3.pl"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
    </rule>

    <rule weight="2">
      <match tags="vbmod.*.p1.sg"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
      <match tags="prn.enc.p1.mf.sg"/>
    </rule>
      <rule weight="2">
      <match tags="vbmod.*.p2.sg"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
      <match tags="prn.enc.p2.mf.sg"/>
    </rule>
    <rule weight="1">
      <match tags="vbmod.*.p3.sg"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
      <match tags="prn.enc.ref.p3.mf.sp"/>
    </rule>
    <rule weight="2">
      <match tags="vbmod.*.p1.pl"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
      <match tags="prn.enc.p1.mf.pl"/>
    </rule>
      <rule weight="2">
      <match tags="vbmod.*.p2.pl"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
      <match tags="prn.enc.p2.mf.pl"/>
    </rule>
    <rule weight="2">
      <match tags="vbmod.*.p3.pl"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
      <match tags="prn.enc.ref.p3.mf.sp"/>
    </rule>

    <rule weight="2">
      <match tags="prn.pro.p1.mf.sg"/>
      <match tags="vbhaver.*.p1.sg"/>
      <match lemma="{{p1}}" tags="vblex.pp.m.sg"><select lemma="{{p2}}"/></match>
    </rule>
      <rule weight="2">
      <match tags="prn.pro.p2.mf.sg"/>
      <match tags="vbhaver.*.p2.sg"/>
      <match lemma="{{p1}}" tags="vblex.pp.m.sg"><select lemma="{{p2}}"/></match>
    </rule>
    <rule weight="1">
      <match tags="prn.pro.ref.p3.mf.sp"/>
      <match tags="vbhaver.*.p3.sg"/>
      <match lemma="{{p1}}" tags="vblex.pp.m.sg"><select lemma="{{p2}}"/></match>
    </rule>
    <rule weight="2">
      <match tags="prn.pro.p1.mf.pl"/>
      <match tags="vbhaver.*.p1.pl"/>
      <match lemma="{{p1}}" tags="vblex.pp.m.sg"><select lemma="{{p2}}"/></match>
    </rule>
    <rule weight="2">
      <match tags="prn.pro.p2.mf.pl"/>
      <match tags="vbhaver.*.p2.pl"/>
      <match lemma="{{p1}}" tags="vblex.pp.m.sg"><select lemma="{{p2}}"/></match>
    </rule>
      <rule weight="2">
      <match tags="prn.pro.ref.p3.mf.sp"/>
      <match tags="vbhaver.*.p3.pl"/>
      <match lemma="{{p1}}" tags="vblex.pp.m.sg"><select lemma="{{p2}}"/></match>
    </rule>
  </def-macro>

  <def-macro name="verbs_reflexius_no_impersonal"> <!-- traducció del verb si va acompanyat per un pronom reflexiu, excepte 3a persona-->
    <rule weight="2">
      <match tags="prn.pro.p1.mf.sg"/>
      <match lemma="{{p1}}" tags="vblex.*.p1.sg"><select lemma="{{p2}}"/></match>
    </rule>
    <rule weight="2">
      <match tags="prn.pro.p2.mf.sg"/>
      <match lemma="{{p1}}" tags="vblex.*.p2.sg"><select lemma="{{p2}}"/></match>
    </rule>
    <rule weight="2">
      <match tags="prn.pro.p1.mf.pl"/>
      <match lemma="{{p1}}" tags="vblex.*.p1.pl"><select lemma="{{p2}}"/></match>
    </rule>
    <rule weight="2">
      <match tags="prn.pro.p2.mf.pl"/>
      <match lemma="{{p1}}" tags="vblex.*.p2.pl"><select lemma="{{p2}}"/></match>
    </rule>

    <rule weight="2">
      <match tags="prn.pro.p1.mf.sg"/>
      <match tags="*.*.p1.sg"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
    </rule>
      <rule weight="2">
      <match tags="prn.pro.p2.mf.sg"/>
      <match tags="*.*.p2.sg"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
    </rule>
    <rule weight="2">
      <match tags="prn.pro.p1.mf.pl"/>
      <match tags="*.*.p1.pl"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
    </rule>
      <rule weight="2">
      <match tags="prn.pro.p2.mf.pl"/>
      <match tags="vbmod.*.p2.pl"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
    </rule>

    <rule weight="2">
      <match tags="*.*.p1.sg"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
      <match tags="prn.enc.p1.mf.sg"/>
    </rule>
      <rule weight="2">
      <match tags="*.*.p2.sg"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
      <match tags="prn.enc.p2.mf.sg"/>
    </rule>
    <rule weight="2">
      <match tags="*.*.p1.pl"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
      <match tags="prn.enc.p1.mf.pl"/>
    </rule>
      <rule weight="2">
      <match tags="*.*.p2.pl"/>
      <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p2}}"/></match>
      <match tags="prn.enc.p2.mf.pl"/>
    </rule>

    <rule weight="2">
      <match tags="prn.pro.p1.mf.sg"/>
      <match tags="vbhaver.*.p1.sg"/>
      <match lemma="{{p1}}" tags="vblex.pp.m.sg"><select lemma="{{p2}}"/></match>
    </rule>
      <rule weight="2">
      <match tags="prn.pro.p2.mf.sg"/>
      <match tags="vbhaver.*.p2.sg"/>
      <match lemma="{{p1}}" tags="vblex.pp.m.sg"><select lemma="{{p2}}"/></match>
    </rule>
    <rule weight="2">
      <match tags="prn.pro.p1.mf.pl"/>
      <match tags="vbhaver.*.p1.pl"/>
      <match lemma="{{p1}}" tags="vblex.pp.m.sg"><select lemma="{{p2}}"/></match>
    </rule>
      <rule weight="2">
      <match tags="prn.pro.p2.mf.pl"/>
      <match tags="vbhaver.*.p2.pl"/>
      <match lemma="{{p1}}" tags="vblex.pp.m.sg"><select lemma="{{p2}}"/></match>
    </rule>
  </def-macro>
</def-macros>

<rules>

<!-- **************** NUM **************** -->

<!-- uno > un, u -->
  <rule weight="0.1"> 
    <match lemma="uno" tags="num.*">
        <select lemma="un" />
    </match>
  </rule>
  <rule weight="1.0">
    <match lemma="número"/>
    <match lemma="uno" tags="num.*">
        <select lemma="u" />
    </match>
  </rule>

<!-- **************** ADJ **************** -->

  <rule weight="0.5">
     <match lemma="querido" tags="adj.*"><select lemma="estimat"/></match>
  </rule>
  <rule weight="2.0">
     <match lemma="querido" tags="adj.*"><select lemma="benvolgut"/></match>
     <match tags="n.*"/>
  </rule>

  <rule weight="1.0">
     <match lemma="querido" tags="adj.*"><select lemma="benvolgut"/></match>
     <match tags="adj.*"/>
     <match tags="n.*"/>
  </rule>


  <rule weight="0.5">
     <match lemma="vario" tags="adj.*"><select lemma="vari"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="vario" tags="adj.*.pl"><select lemma="divers"/></match>
     <match tags="n.*"/>
  </rule>
  <rule weight="1.0">
     <match lemma="vario" tags="adj.*.pl"><select lemma="divers"/></match>
     <match tags="adj.*"/>
     <match tags="n.*"/>
  </rule>

<!-- **************** N **************** -->

<!-- mesa > taula, mesa -->

  <rule weight="0.5"> 
    <match lemma="mesa" tags="n.*"><select lemma="taula" /></match>
  </rule>

  <rule weight="1"> <!-- en majúscula i sense context: "la Mesa (del Parlamento)"-->
    <match lemma="Mesa" tags="n.f.sg"><select lemma="mesa" /></match>
  </rule>


  <!--<rule weight="1.5">  serviria si es pogués marcar que és principi de frase
    <match lemma="Mesa" tags="n.f.sg"><select lemma="taula" /></match>
    <or>
      <match lemma="de"/>
      <match tags="adj.f.sg"/>
      <match tags="adj.mf.sg"/>
      <match tags="adj.f.sp"/>
      <match tags="adj.mf.sp"/>
    </or>
  </rule>-->


  <rule weight="3">
    <match lemma="uva"/>
    <match lemma="de"/>
    <match lemma="mesa" tags="n.*"><select lemma="taula" /></match>
  </rule>

  <rule weight="5">
    <or>
      <match lemma="continuar"/>
      <match lemma="estar"/>
      <match lemma="permanecer"/>
      <match lemma="poner"/>
      <match lemma="proseguir"/>
      <match lemma="seguir"/>
    </or>
    <or>
      <match lemma="sobre"/>
      <match lemma="encima de"/>
    </or>
    <match tags="det.*"/>
    <match lemma="mesa" tags="n.*"><select lemma="taula" /></match>
  </rule>

  <rule weight="3"> 
    <match lemma="mesa" tags="n.*"><select lemma="taula" /></match>
    <or>
      <match lemma="general"/>
      <match lemma="negociador"/>
      <match lemma="quirúrgico"/>
      <match lemma="redondo"/>
      <match lemma="sectorial"/>
      <match lemma="territorial"/>
      <match lemma="técnico"/>
    </or>
  </rule>

  <rule weight="3"> 
    <match lemma="mesa" tags="n.*"><select lemma="taula" /></match>
    <repeat from="0" upto="1">
      <match tags="adj.*"/>
    </repeat>
    <match tags="pr"/>
    <or>
      <match lemma="autónomo"/>
      <match lemma="cambio"/>
      <match lemma="corte"/>
      <match lemma="control"/>
      <match lemma="cultivo"/>
      <match lemma="debate"/>
      <match lemma="discusión"/>
      <match lemma="estudio"/>
      <match lemma="negociación"/>
      <match lemma="operación"/>
      <match lemma="trabajo"/>
      <match lemma="quirófano"/>
    </or>
  </rule>

  <rule weight="2"> 
    <match lemma="mesa" tags="n.*"><select lemma="mesa" /></match>
    <match lemma="electoral"/>
  </rule>

  <rule weight="2"> 
    <match lemma="mesa" tags="n.*"><select lemma="mesa" /></match>
    <match lemma="de"/>
    <or>
      <match lemma="contratación"/>
      <match lemma="edad"/>
      <match lemma="les Corts"/>
    </or>
  </rule>

  <rule weight="2"> 
    <match lemma="mesa" tags="n.*"><select lemma="mesa" /></match>
    <match lemma="de"/>
    <match tags="det.*"/>
    <or>
      <match lemma="corte" tags="n.f.pl"/>
      <match lemma="parlamento"/>
      <match lemma="congreso"/>
      <match lemma="asamblea"/>
      <match lemma="cámara"/>
      <match lemma="presidencia"/>
      <!--<match lemma="convención"/>-->
      <!--<match lemma="consejo"/>-->
      <!--<match lemma="presidente"/>-->
      <match lemma="senado"/>
      <match lemma="Cortes Valencianas"/>
      <match lemma="Corts Valencianes"/>
    </or>
  </rule>

  <rule weight="2">
    <or>
      <match lemma="presidente"/>
      <match lemma="vicepresidente"/>
      <match lemma="secretario"/>
      <match lemma="vicesecretario"/>
      <match lemma="subsecretario"/>
      <match lemma="vocal"/>
      <match lemma="letrado"/>
    </or>
    <match lemma="de"/>
    <match tags="det.def.*"/>
    <match lemma="mesa" tags="n.*"><select lemma="mesa" /></match>    
  </rule>

<!-- trabajo > treball, feina/faena -->

  <rule weight="0.5"> 
    <match lemma="trabajo" tags="n.*"><select lemma="treball" /></match>
  </rule>

  <rule weight="2"> 
    <or>
      <match lemma="mucho"/>
      <match lemma="buen"/>
      <match lemma="gran"/>
      <match lemma="algo de"/>
    </or>
    <match lemma="trabajo" tags="n.m.sg"><select lemma="feina" /></match>
  </rule>

<!-- fallo > fallada, dispositiva, decisió -->

  <rule weight="0.5"> 
    <match lemma="fallo" tags="n.*"><select lemma="fallada" /></match>
  </rule>

  <rule weight="2"> 
    <or>
      <match lemma="encabezamiento"/>
      <match lemma="sentencia"/>
    </or>
    <match lemma="y"/>
    <match lemma="fallo" tags="n.*"><select lemma="part dispositiva" /></match>
  </rule>

  <rule weight="2"> 
    <match lemma="fallo" tags="n.*"><select lemma="part dispositiva" /></match>
    <repeat from="0" upto="3">
      <match tags="*"/>
    </repeat>
    <or>
      <match lemma="sentencia"/>
      <match lemma="sentenciar"/>
      <match lemma="parte dispositiva"/>
      <match lemma="dispositiva"/>
    </or>
  </rule>

  <rule weight="2"> 
    <or>
      <match lemma="sentencia"/>
      <match lemma="sentenciar"/>
      <match lemma="parte dispositiva"/>
      <match lemma="dispositiva"/>
    </or>
    <repeat from="0" upto="3">
      <match tags="*"/>
    </repeat>
    <match lemma="fallo" tags="n.*"><select lemma="part dispositiva" /></match>
  </rule>

  <rule weight="1"> 
    <match lemma="fallo" tags="n.*"><select lemma="decisió" /></match>
    <repeat from="0" upto="3">
      <match tags="*"/>
    </repeat>
    <match lemma="jurado"/>
  </rule>

  <rule weight="1"> 
    <match lemma="fallo" tags="n.*"><select lemma="part dispositiva" /></match>
    <repeat from="0" upto="3">
      <match tags="*"/>
    </repeat>
    <or>
      <match lemma="juez"/>
      <match lemma="audiencia"/>
    </or>
  </rule>


<!-- corte > corte, tall -->
  <rule weight="0.1"> 
    <match lemma="corte" tags="n.*">
        <select lemma="cort" />
    </match>
  </rule>
  <rule weight="1.0"> 
    <match lemma="*" tags="det.*.f.*"/>
    <match lemma="corte" tags="n.*">
        <select lemma="cort" />
    </match>
  </rule>
  <rule weight="1.0"> 
    <match lemma="corte" tags="n.*">
        <select lemma="cort" />
    </match>
    <match lemma="*" tags="adj.f.*"/>
  </rule>


  <rule weight="1.0">
    <match lemma="*" tags="det.*.m.*"/>
    <match lemma="corte" tags="n.*">
        <select lemma="tall" />
    </match>
  </rule>
  <rule weight="0.5"> <!--de corte -->
    <match lemma="de"/>
    <match lemma="corte" tags="n.mf.sg">
        <select lemma="tall" />
    </match>
  </rule>

  <rule weight="2"> <!--de corte -->
    <or>
      <match lemma="pintor"/>
      <match lemma="pintura"/>
      <match lemma="ropa"/>
      <match lemma="vestido"/>
    </or>
    <match lemma="de"/>
    <match lemma="corte" tags="n.mf.sg"><select lemma="cort" /></match>
  </rule>

  <rule weight="2"> <!--de corte -->
    <match lemma="corte" tags="n.mf.sg"><select lemma="cort" /></match>
    <match lemma="y"/>
    <match lemma="confección"/>
  </rule>

  <rule weight="1.0">  <!-- los continuos cortes -->
    <match lemma="*" tags="adj.m.*"/>
    <match lemma="corte" tags="n.*">
        <select lemma="tall" />
    </match>
  </rule>
  <rule weight="1.0"> 
    <match lemma="corte" tags="n.*">
        <select lemma="tall" />
    </match>
    <match lemma="*" tags="adj.m.*"/>
  </rule>
  <rule weight="1.0">  <!-- un monumental corte -->
    <match lemma="*" tags="det.*.m.*"/>
    <match lemma="*" tags="adj.mf.*"/>
    <match lemma="corte" tags="n.*">
        <select lemma="tall" />
    </match>
  </rule>
  <rule weight="1.0"> 
    <match lemma="corte" tags="n.*"><select lemma="tall" /></match>
    <repeat from="0" upto="2">
      <or>
        <match lemma="*" tags="det.*"/>
        <match lemma="de"/>
      </or>
    </repeat>
    <or>
      <match lemma="vía"/>
      <match lemma="luz"/>
      <match lemma="tráfico"/>
      <match lemma="tránsito"/>
      <match lemma="suministro"/>
      <match lemma="carretera"/>
      <match lemma="electricidad"/>
      <match lemma="energía"/>
      <match lemma="digestión"/>
      <match lemma="pelo"/>
      <match lemma="cabello"/>
    </or>
