  <!-- cesar > cessar / destituir -->

  <rule weight="0.5">
    <match lemma="cesar" tags="vblex.*"><select lemma="cessar"/></match>
  </rule>

  <rule weight="1">
    <match lemma="ser" tags="vbser.*"/>
    <match lemma="cesar" tags="vblex.pp.*"><select lemma="destituir"/></match>
  </rule>

  <rule weight="1">
    <match lemma="cesar" tags="vblex.*"><select lemma="destituir"/></match>
    <repeat from="1" upto="3">
      <or>
        <match tags="det.*"/>
        <match tags="pr" lemma="a"/>
        <match tags="num"/>
        <match tags="num.*"/>
      </or>
    </repeat>
    <or>
      <match tags="n.*" lemma="ministro"/>
      <match tags="n.*" lemma="consejero"/>
      <match tags="n.*" lemma="presidente"/>
      <match tags="n.*" lemma="director"/>
      <match tags="n.*" lemma="rector"/>
      <match tags="n.*" lemma="cargo"/>
      <match tags="n.*" lemma="miembro"/>
    </or>
  </rule>

  <!-- qué duda cabe > no hi ha dubte / no n'hi ha dubte -->

  <rule weight="0.5">
    <match lemma="qué duda cabe" tags="adv"><select lemma="no n'hi ha dubte"/></match>
  </rule>

  <rule weight="1">
    <match lemma="qué duda cabe" tags="adv"><select lemma="no hi ha dubte"/></match>
    <or>
      <match lemma="de" tags="pr"/>
      <match lemma="que" />
      <match lemma="de que"/>
    </or>
  </rule>

  <!-- por entonces > en aquells dies / llavors -->

  <rule weight="0.5">
    <match lemma="por entonces" tags="adv"><select lemma="en aquells dies"/></match>
  </rule>

  <rule weight="1">
    <match tags="det.*"/>
    <match lemma="por entonces" tags="adv"><select lemma="llavors"/></match>
  </rule>
  

  <!-- berenjenal > embolic / vesper / camp d'albergínies  -->

  <rule weight="0.5">
    <match lemma="berenjenal" tags="n.*"><select lemma="embolic"/></match>
  </rule>

  <rule weight="1">
    <or>
      <match lemma="meter"/>
      <match lemma="entrar"/>
    </or>
    <repeat from="0" upto="5">
      <match tags="*"/>
    </repeat>
    <match lemma="berenjenal" tags="n.*"><select lemma="vesper"/></match>
  </rule>


<!-- puesto que > ja que / com que-->

  <rule weight="0.5">
    <match lemma="puesto que" tags="cnjadv"><select lemma="ja que"/></match>
  </rule>

  <rule weight="1"> <!-- quan no hi ha coma -->
    <or>
      <match tags="n.*"/>
      <match tags="adj.*"/>
      <match tags="vblex.*"/>
      <match tags="adv"/>
    </or>
    <match lemma="puesto que" tags="cnjadv"><select lemma="perquè"/></match>
  </rule>

  <rule weight="2">
    <or>
      <match tags="cnjcoo"/>
      <match lemma="que"/>
    </or>
    <match lemma="puesto que" tags="cnjadv"><select lemma="com que"/></match>
  </rule> 
  <rule weight="2">
    <match lemma="que"/>
    <match tags="cm"/>
    <match lemma="puesto que" tags="cnjadv"><select lemma="com que"/></match>
  </rule> 

  <rule weight="4">
    <match lemma="Puesto que" tags="cnjadv"><select lemma="com que"/></match>
  </rule> 

<!-- D. > Sr. / D.  -->

  <rule weight="0.5">
    <match lemma="D." tags="n.*"><select lemma="D."/></match>
  </rule>

  <rule weight="1">
    <match lemma="D." tags="n.*"><select lemma="Sr."/></match>
    <match tags="np.*"/>
  </rule> 
  
  <!-- algo > alguna cosa / una cosa -->

  <rule weight="0.5">
    <match lemma="algo" tags="prn.*"><select lemma="alguna cosa"/></match>
  </rule>

  <rule weight="1">
    <match lemma="algo" tags="prn.*"><select lemma="una cosa"/></match>
    <match lemma="así" tags="adv"/>
    <!--<match lemma="como" tags="adv"/>-->
  </rule>

  <rule weight="1">
    <match lemma="algo" tags="prn.*"><select lemma="una cosa"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="preadv"/>
      </or>
    </repeat>
    <or>
      <match tags="adj.m.sg"/>
      <match tags="adj.mf.sg"/>
      <match tags="vblex.pp.m.sg"/>
    </or>
  </rule>

  <!-- Fausto > Faust /Fausto  -->

  <rule weight="5">
    <or>
      <match lemma="doctor"/>
      <match lemma="Dr"/>
      <match lemma="Dr."/>
      <match lemma="nuevo"/>
    </or>
    <match lemma="Fausto" tags="np.*"><select lemma="Faust"/></match>
  </rule>

  <rule weight="5">
    <or>
      <match lemma="leyenda"/>
      <match lemma="Goethe"/>
      <match lemma="Gretchen"/>
      <match lemma="Margarita"/>
      <match lemma="pacto"/>
      <match lemma="diablo"/>
    </or>
    <repeat form="0" upto="8">
      <match tags="*"/>
    </repeat>
    <match lemma="Fausto" tags="np.*"><select lemma="Faust"/></match>
  </rule>

  <rule weight="5">
    <match lemma="Fausto" tags="np.*"><select lemma="Faust"/></match>
    <repeat form="0" upto="8">
      <match tags="*"/>
    </repeat>
    <or>
      <match lemma="leyenda"/>
      <match lemma="Goethe"/>
      <match lemma="Gretchen"/>
      <match lemma="Margarita"/>
      <match lemma="pacto"/>
      <match lemma="diablo"/>
    </or>
  </rule>
  

