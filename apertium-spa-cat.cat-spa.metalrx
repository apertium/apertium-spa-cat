<rules glob="plus">

<!-- **************** PR **************** -->
  <rule weight="1">
   <match lemma="de" tags="pr"><select lemma="de"/></match>
  </rule>

  <rule weight="2">
    <match lemma="endemà" tags="n.m.sg"><select lemma="día siguiente"/></match>
    <match lemma="de" tags="pr"><select lemma="a"/></match>
  </rule>

<!-- a > que -->

  <rule weight="2">
    <match lemma="tenir"/>
    <match lemma="a" tags="pr"><select lemma="que" tags="cnjsub"/></match>
    <match lemma="veure"/>
  </rule>

<!-- a > a, en -->
  <rule weight="0.6">
    <match lemma="a" tags="pr"><select lemma="a" tags="pr"/></match>
  </rule>
  <rule weight="2.0">
    <or>
      <seq n="regeix_a"/>
    </or>
    <match lemma="a" tags="pr"><select lemma="a" tags="pr"/></match>
  </rule>


  
  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="en" tags="pr"/></match>
    <or><match tags="np.loc"/><match tags="np.top.*"/><match tags="np.hyd.*"/><match tags="np.al"/><match tags="np.al.*"/></or>
  </rule>
  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="en" tags="pr"/></match>
    <match tags="det.*"/>
    <or><match tags="np.loc"/><match tags="np.top.*"/><match tags="np.hyd.*"/><match tags="np.al"/><match tags="np.al.*"/></or>
  </rule>
  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="en" tags="pr"/></match>
    <match tags="det.*"/>
    <or>
      <seq n="punt_cardinal"/>
