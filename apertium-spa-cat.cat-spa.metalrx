  </rule>
  <rule weight="1.0">
     <match lemma="caragol" tags="n.m.*"><select lemma="tornillo"/></match>
     <or><match tags="np.cog.*"/><match tags="np.al.*"/></or> <!-- Philips, Allen, etc. -->
  </rule>
  <rule weight="1.0">
     <match lemma="cargol" tags="n.m.*"><select lemma="tornillo"/></match>
     <or>
       <match lemma="pla" tags="adj.*"/>
       <match lemma="quadrat" tags="adj.*"/>
       <match lemma="hexagonal" tags="adj.*"/>
     </or>
  </rule>
  <rule weight="1.0">
     <match lemma="cargol" tags="n.m.*"><select lemma="tornillo"/></match>
     <match lemma="en" tags="pr"/>
     <match lemma="estrella" tags="n.*"/>
  </rule>
  <rule weight="1.0">
     <match lemma="cargol" tags="n.m.*"><select lemma="tornillo"/></match>
     <match lemma="de" tags="pr"/>
     <match lemma="pala" tags="n.*"/>
  </rule>
  <rule weight="1.0">
     <match lemma="cargol" tags="n.m.*"><select lemma="tornillo"/></match>
     <or><match tags="np.cog.*"/><match tags="np.al.*"/></or> <!-- Philips, Allen, etc. -->
  </rule>

<!-- tret > caraterística, disparo -->

  <rule weight="0.6">
     <match lemma="tret" tags="n.*"><select lemma="caraterística"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="tret" tags="n.*"><select lemma="disparo"/></match>
     <match lemma="a" tags="pr"/>
  </rule>

<!-- cap > jefe, cabo, cabeza, capital -->

  <rule weight="0.6">
     <match lemma="cap" tags="n.*"><select lemma="jefe"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="cap" tags="n.*"><select lemma="cabo"/></match>
     <or><match tags="np.loc"/><match tags="np.top.*"/></or>
  </rule>
  <rule weight="1.0">
     <match lemma="cap" tags="n.*"><select lemma="cabo"/></match>
     <match lemma="de" tags="pr"/>
     <or><match tags="np.loc"/><match tags="np.top.*"/></or>
  </rule>
  <rule weight="1.0">
     <match lemma="cap" tags="n.*"><select lemma="capital"/></match>
     <match lemma="de" tags="pr"/>
     <repeat from="0" upto="1">
       <match tags="det.*"/>
     </repeat>
    <match lemma="comarca" tags="n.*"/>
  </rule>
  <rule weight="1.0">
     <match lemma="cap" tags="n.*"><select lemma="cabeza"/></match>
     <match lemma="de" tags="pr"/>
     <repeat from="0" upto="1">
       <match tags="det.*"/>
     </repeat>
     <or>
      <match lemma="raqueta" tags="n.*"/>
      <match lemma="llista" tags="n.*"/>
      <match lemma="municipi" tags="n.*"/>

      <seq n="animal"/>
      <seq n="material"/>
     </or>
  </rule>

<!-- taula > mesa, tabla -->

  <rule weight="0.6">
     <match lemma="taula" tags="n.*"><select lemma="mesa"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="taula" tags="n.*"><select lemma="tabla"/></match>
     <match lemma="de" tags="pr"/>
<!--
     <or>
       <match lemma="multiplicar" tags="n.*"/>
       <match lemma="dada" tags="n.*"/>
       <match lemma="còmput" tags="n.*"/>
       <match lemma="ordenació" tags="n.*"/>
       <match lemma="cas" tags="n.*"/>
       <match lemma="símbol" tags="n.*"/>
       <match lemma="literal" tags="n.*"/>
       <match lemma="Pitàgores" tags="np.*"/>
       <match lemma="Mendeléiev" tags="np.*"/>
       <match lemma="Huffman" tags="np.*"/>
     </or>
-->
  </rule>
  <rule weight="1.0">
     <match lemma="taula" tags="n.*"><select lemma="tabla"/></match>
     <match lemma="que"/>
  </rule>
  <rule weight="1.2">
     <match lemma="taula" tags="n.*"><select lemma="mesa"/></match>
     <match lemma="de" tags="pr"/>
     <or>
       <match lemma="treball" tags="n.*"/>
       <match lemma="nit" tags="n.*"/>
     </or>
  </rule>
  <rule weight="1.0">
     <match lemma="taula" tags="n.*"><select lemma="tabla"/></match>
     <or>
       <match lemma="periòdic" tags="adj.*"/>
       <match lemma="astronòmic" tags="adj.*"/>
       <match lemma="cronològic" tags="adj.*"/>
       <match lemma="següent" tags="adj.*"/>
       <match lemma="anterior" tags="adj.*"/>
       <match lemma="superior" tags="adj.*"/>
       <match lemma="inferior" tags="adj.*"/>
       <match lemma="esquemàtica" tags="adj.*"/>
       <match lemma="número"/>
       <match lemma="núm."/>
       <match lemma="no."/>
       <match tags="num"/>
     </or>
  </rule>
  <rule weight="1.0">
     <or>
       <match lemma="següent" tags="adj.*"/>
       <match lemma="anterior" tags="adj.*"/>
     </or>
     <match lemma="taula" tags="n.*"><select lemma="tabla"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="taula" tags="n.*"><select lemma="tabla"/></match>
     <or>
       <match lemma="mostrar" tags="vblex.*"/>
       <match lemma="resumir" tags="vblex.*"/>
     </or>
  </rule>
  <rule weight="1.0">
     <match lemma="taula" tags="n.*"><select lemma="tabla"/></match>
     <!-- <match lemma="es" tags="prn.pro.ref.*"/> -->
     <match tags="*"/> <!-- prn, adv, adj, num -->
     <or>
       <match lemma="mostrar" tags="vblex.*"/>
       <match lemma="veure" tags="vblex.*"/>
       <match lemma="resumir" tags="vblex.*"/>
     </or>
  </rule>

    <!-- deslliurar > liberar/librar de... -->

  <rule weight="0.6">
    <match lemma="deslliurar" tags="vblex.*"><select lemma="liberar"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="deslliurar" tags="vblex.*"><select lemma="librar"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="de"/>  
  </rule>

  <rule weight="1.0">
    <match lemma="en" tags="prn.*"/>
    <repeat from="0" upto="1">
      <match lemma="anar" tags="vblex.*"/>
    </repeat>
    <match lemma="deslliurar" tags="vblex.*"><select lemma="librar"/></match>
  </rule>

  <!-- recórrer > recorrer/recurrir a... -->

  <rule weight="0.6">
    <match lemma="recórrer" tags="vblex.*"><select lemma="recorrer"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="recórrer" tags="vblex.*"><select lemma="recurrir"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="a"/>  
  </rule>

    <!-- llit > cama  / llecho (de río ... -->

  <rule weight="0.5">
    <match lemma="llit" tags="n.*"><select lemma="cama"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="llit" tags="n.*"><select lemma="lecho"/></match>
    <repeat from="0" upto="3">
      <match tags="*" />
    </repeat>
    <or>
      <match lemma="riu" tags="n.*"/>
      <match lemma="mar" tags="n.*"/>
      <match lemma="barranc" tags="n.*"/>
      <match lemma="canal" tags="n.*"/>
      <match lemma="ria" tags="n.*"/>
      <match lemma="rierol" tags="n.*"/>
      <match lemma="corrent" tags="n.*"/>
      <match lemma="roca" tags="n.*"/>
      <match lemma="pedra" tags="n.*"/>
      <match lemma="llac" tags="n.*"/>
      <match lemma="estany" tags="n.*"/>
      <match lemma="marí" tags="adj.*"/>
      <match lemma="fluvial" tags="adj.*"/>
      <match lemma="seco" tags="adj.*"/>
      <match lemma="rocós" tags="adj.*"/>
      <match lemma="Túria"/>
      <match lemma="Xúquer"/>
      <match lemma="Segura"/>
      <match lemma="Ebre"/>
      <match lemma="Tajo"/>
      <match lemma="Vinalopó"/>
      <match lemma="Duero"/>      
    </or>
  </rule>

  <!-- posar > poner, posar -->

  <rule weight="0.6">
     <match lemma="posar" tags="vblex.*"><select lemma="poner"/></match>
  </rule>  

  <!-- grau > grado, grao -->

  <rule weight="0.6">
     <match lemma="grau" tags="n.*"><select lemma="grado"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="grau" tags="n.*"><select lemma="grao"/></match>
     <match lemma="de" tags="pr"/>
     <or>
       <match lemma="València" tags="np.*"/>
       <match lemma="Gandia" tags="np.*"/>
       <match lemma="Castelló" tags="np.*"/>
       <match lemma="Moncofa" tags="np.*"/>
     </or>
  </rule>

  <rule weight="1.0">
    <or>
       <match lemma="carretera" tags="n.*"/>
       <match lemma="camí" tags="n.*"/>
       <match lemma="via" tags="n.*"/>
       <match lemma="sud" tags="n.*"/>
       <match lemma="nord" tags="n.*"/>
       <match lemma="est" tags="n.*"/>
       <match lemma="oest" tags="n.*"/>
       <match lemma="fondo" tags="n.*"/>
    </or>
    <repeat from="0" upto="3">
      <or>
        <match tags="pr"/>
        <match tags="det.*"/>
        <match tags="adj.*"/>
      </or>
    </repeat>
    <match lemma="grau" tags="n.*"><select lemma="grao"/></match>
  </rule>

  <!-- gana > gana / hambre -->

  <rule weight="0.5">
    <match lemma="gana" tags="n.f.sg"><select lemma="hambre"/></match>
  </rule>

  <rule weight="0.5">
    <match lemma="gana" tags="n.f.pl"><select lemma="gana"/></match>
  </rule>
  

<!-- **************** NP **************** -->

<!-- Virgili > Virgili, Virgilio -->

  <rule weight="0.6">
     <match lemma="Virgili" tags="np.*"><select lemma="Virgilio"/></match>
  </rule>
  <rule weight="1.0">
     <or><match tags="np.ant.*"/><match tags="np.cog.*"/></or>
     <match lemma="Virgili" tags="np.*"><select lemma="Virgili"/></match>
  </rule>
  <rule weight="1.0">
     <or><match tags="np.cog.*"/><match lemma="Rovira"/></or>
     <match lemma="i" tags="pr"/>
     <match lemma="Virgili" tags="np.*"><select lemma="Virgili"/></match>
  </rule>

